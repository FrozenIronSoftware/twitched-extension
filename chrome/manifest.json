{
  "name": "Twitched",
  "description": "__MSG_description__",
  "version": "1.3.2",
  "manifest_version": 2,
  "author": "Rolando Islas",
  "homepage_url": "https://www.twitched.org",
  "offline_enabled": false,
  "options_ui": {
    "page": "src/options.html",
    "chrome_style": true
  },
  "default_locale": "en_US",
  "icons": {
    "16": "assets/image/icon_16.png",
    "32": "assets/image/icon_32.png",
    "128": "assets/image/icon_128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "assets/image/icon_16.png",
      "32": "assets/image/icon_32.png",
      "128": "assets/image/icon_128.png"
    }
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "notifications",
    "storage",
    "https://*.twitch.tv/*",
    "https://twitch.tv/*",
    "http://*/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "src/extract_time.js"
  ]
}